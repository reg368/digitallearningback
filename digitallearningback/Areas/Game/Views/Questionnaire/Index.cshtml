@using digitallearningback.Models
@{ InfoUser user = InfoUser.getLoginUser(); }
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>遊戲式練習系統問卷調查</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/Scripts/angular/angular.js")
    @Scripts.Render("~/Scripts/angular-route/angular-route.js")
    @Scripts.Render("~/Scripts/Game/Questionnaire/app.module.js")
    @Scripts.Render("~/Scripts/Game/Questionnaire/app.config.js")
    @Scripts.Render("~/Scripts/Game/Questionnaire/questionnaireIndex/questionnaire-index.module.js")
    @Scripts.Render("~/Scripts/Game/Questionnaire/questionnaireIndex/questionnaire-index.component.js")
</head>
<body ng-app="QuestionnaireApp">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="#" class="navbar-brand">遊戲式練習系統問卷調查</a>
            </div>
            <div class="navbar-collapse collapse">

            </div>
        </div>
    </div>
    <div class="container body-content">
        

        <div ng-view></div>


        <hr />
        <footer>
            <!--<p>&copy; @DateTime.Now.Year - 我的 ASP.NET 應用程式</p>-->
            <p>@user.name 您好 - 歡迎進入遊戲式練習系統問卷調查</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

</body>
</html>


